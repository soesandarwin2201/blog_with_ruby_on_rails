<section id='user-container'>
<img src="<%= @user.photo %>" alt="image">
<h1><%= @user.name%></h1>
<p>Number of post: <%= @user.posts.length%></p>
</section>
<p id='user-bio'><%= @user.bio %></>
<div class='post-container'>
<% @posts.each_with_index do |post,index| %>
<li>
<p>Post #<%= index + 1%></p>
 <h3><%= link_to post.title, controller: "posts", action: "show", user_id: @user.id, post_id: post.id %></h3>
<p>Comments: <%= post.commentscounter %></p>
<p>Likes: <%= post.likescounter %></p>
</li>
<% end %>
<li>
<button><%= link_to 'See all the posts', controller: 'posts', action: 'index', user_id: @user.id %></button>
</li>
</div>